bmi <- function(t,w){
x<- w%/%((t*t)%/%10000)
res <- if(x>=35)"고도 비만" else
if(x>=30 & x<35)"중등도 비만"else
if(x>=25 & x<30)"경도 비만"else
if(x>=23 & x<24.9)"과체중"else
if(x>=18.5 & x<22.9)"정상"else
"저체중"
return(res)
}
